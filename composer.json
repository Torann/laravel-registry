{
    "name": "torann/registry",
    "description": "Laravel registry manager for application configurations",
    "keywords": ["registry", "laravel", "settings"],
	"license": "BSD 2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.1|~5.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Torann\\Registry": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}